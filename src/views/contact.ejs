<%- include('./header.ejs') %>
  <div class="row mx-0 flex-nowrap">
    <%- include('./nav.ejs') %>
      <div class="col py-3">
        <h1>
          <%= contact %>
        </h1>
        <table class="table table-bordered">
          <thead class="bg-primary text-white">
            <tr>
              <th>名字</th>
              <th>姓氏</th>
              <th>手機</th>
              <th>Email</th>
              <th>內容</th>
              <th class="width-date-time">留言時間</th>
            </tr>
          </thead>
          <tbody>
            <% data.map((each)=> { %>
              <tr>
                <td class="text-nowrap">
                  <%= each.firstName %>
                </td class="text-nowrap">
                <td>
                  <%= each.lastName %>
                </td>
                <td>
                  <%= each.mobile %>
                </td>
                <td>
                  <%= each.email %>
                </td>
                <td>
                  <%= each.content %>
                </td>
                <td>
                  <%= each.time %>
                </td>
              </tr>
              <% }) %>
          </tbody>
        </table>
        <%- include('./pagination.ejs') %>
      </div>
  </div>
  <%- include('./footer.ejs') %>